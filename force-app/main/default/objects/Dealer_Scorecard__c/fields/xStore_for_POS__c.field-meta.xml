<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>xStore_for_POS__c</fullName>
    <description>10 indicates store is on xStore.  0 Indicates store is on other POS.</description>
    <externalId>false</externalId>
    <formula>IF(
ISPICKVAL( Dealer__r.POS_System_Name__c , &apos;XStore - North America&apos;)
,10,0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>10 indicates store is on xStore.  0 Indicates store is on other POS.  Calculated from Account setting.</inlineHelpText>
    <label>xStore for POS</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
