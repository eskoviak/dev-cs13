<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Dealer_Communication_Score__c</fullName>
    <description>Calculates the dealers communication score during Build-out or Remodel</description>
    <externalId>false</externalId>
    <formula>CASE( Dealer_Communication__c , 
&quot;No Issues&quot;,10, 
&quot;Moderate Issues&quot;,5, 
&quot;Significant Issues&quot;,1, 
0)</formula>
    <label>Dealer Communication Score</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
