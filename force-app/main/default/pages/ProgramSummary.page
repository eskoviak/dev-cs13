            function ProgramStatus() {
                $(".modal-backdrop.fade.in").hide();
                var status = '{!ProgramStatus}';
                var error = '{!errorMsg}';
                var exceptionError = '{!ProgramActivationStatus}';
                
                if (exceptionError != '') {
                    specialModal('fa fa-bell-o', 'ACTIVATION ERROR', 'Program is not active. ' + exceptionError, null, '', '$(".modal-backdrop.fade.in").hide()', 'OKAY', null, '');                
                } 
                
                if (error != '') {
                    //$("#ActivationErrorMessageDisplay").show();
                    specialModal('fa fa-bell-o', 'ACTIVATION ERROR', 'Program is not active. ' + error, null, '', '$(".modal-backdrop.fade.in").hide()', 'OKAY', null, '');
                } 
                
                if (exceptionError == '' && error == '') {
